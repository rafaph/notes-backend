{
    "name": "notes-backend",
    "version": "1.4.0",
    "private": true,
    "scripts": {
        "start": "node -r ./boot-paths.js dist/main/server.js",
        "dev": "ts-node-dev --watch=src/**/*.ts --inspect=0.0.0.0:9229 --exit-child --respawn --transpile-only -r tsconfig-paths/register src/main/server.ts",
        "test": "mocha",
        "test:watch": "npm test -- --watch",
        "test:integration": "npm test -- --fgrep @integration",
        "test:integration:watch": "npm run test:integration -- --watch",
        "test:unit": "npm run test:integration -- --invert",
        "test:unit:watch": "npm run test:unit -- --watch",
        "test:ci": "nyc npm test",
        "prelint": "ls-lint",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "lint:fix": "npm run lint -- --fix",
        "prebuild": "rm -rf dist",
        "build": "tsc --project tsconfig-build.json",
        "postbuild": "npm run db:migrate",
        "db:migrate": "sequelize db:migrate"
    },
    "devDependencies": {
        "@ls-lint/ls-lint": "^1.10.0",
        "@types/async": "^3.2.8",
        "@types/chai": "^4.2.22",
        "@types/chai-as-promised": "^7.1.4",
        "@types/chai-datetime": "0.0.37",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/faker": "^5.5.8",
        "@types/lodash": "^4.14.175",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.10.6",
        "@types/pg": "^8.6.1",
        "@types/sinon": "^10.0.4",
        "@types/supertest": "^2.0.11",
        "@types/umzug": "^2.3.2",
        "@types/validator": "^13.6.3",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "async": "^3.2.1",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "chai-datetime": "^1.8.0",
        "connection-string": "^4.3.4",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "faker": "^5.5.3",
        "get-port": "^6.0.0",
        "git-commit-msg-linter": "^3.2.8",
        "husky": "^7.0.2",
        "mocha": "^9.1.2",
        "nyc": "^15.1.0",
        "sequelize-cli": "^6.2.0",
        "sinon": "^11.1.2",
        "supertest": "^6.1.6",
        "ts-node": "^10.3.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "~4.4.4",
        "umzug": "^2.3.0"
    },
    "dependencies": {
        "argon2": "^0.28.2",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "http-status-codes": "^2.1.4",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "pg": "^8.7.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.7.0",
        "tsconfig-paths": "^3.11.0",
        "validator": "^13.6.0",
        "winston": "^3.3.3"
    },
    "engines": {
        "node": "^14"
    }
}
